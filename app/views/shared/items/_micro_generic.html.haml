- canvas_preference ||= false
- external_widget ||= external_widget
.generic.small
  .itemBlock
    - unless external_widget
      = link_to item.title, path_to_self(item)
    - else
      = link_to item.title, path_to_self(item, canvas_preference), :target => "_top"
    .profilePic= local_linked_profile_pic item.user
