#subNav
  .tabs
    %ul#toggle_tabs_unused.toggle_tabs
      %li= link_to t('classifieds.subnav.browse'), classifieds_path, :class => "tab #{sub_tab_selected?(@current_sub_tab, 'Browse')}"
      %li= link_to t('classifieds.subnav.my_items'), my_items_classifieds_path, :class => "tab #{sub_tab_selected?(@current_sub_tab, 'My Items')}"
      %li= link_to t('classifieds.subnav.post_item'), new_classified_path, :class => "tab #{sub_tab_selected?(@current_sub_tab, 'New Item')}"
