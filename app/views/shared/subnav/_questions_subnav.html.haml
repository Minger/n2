#subNav.tabs
  .left_tabs
    %ul#toggle_tabs_unused.toggle_tabs.clearfix
      %li.first= link_to t('.browse_questions'), questions_path, :class => sub_tab_selected?(@current_sub_tab, 'Browse Questions')
      %li.first= link_to t('.ask_question'), new_question_path, :class => sub_tab_selected?(@current_sub_tab, 'Ask Question')
      - if current_user
        %li.first= link_to t('.my_questions'), my_questions_question_path(:id => current_user), :class => sub_tab_selected?(@current_sub_tab, 'My Questions')
