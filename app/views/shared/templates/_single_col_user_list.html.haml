.panel-1
  .panel-bar
    %h2= vt.t(vo.setting.locale_title)
  .users-online
    - unless vo.dataset.empty?
      %ul
        - vo.dataset.each_with_index do |item, index|
          %li
            .profile-pic= local_linked_profile_pic item, :width => '30', :height => '30'
    - else
      %p= vt.t('recent_users_none')
    %div.clearfix