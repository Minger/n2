.panel_1
  .panelBar.clearfix
    %h2= t('.title')
  .subtitle
    %span= t('.subtitle')
  .storyList
    .list_stories.clearfix
      %ul
        - resource_sections.each do |resource_section|
          %li
            .storyBlockWrap
              %p.storyHead= link_to resource_section.name, resource_section
              %p.storyCaption= resource_section.description

  - unless @no_paginate
    .clearfix
      = will_paginate resource_sections
      %br
