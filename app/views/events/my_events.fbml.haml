- content_for :sub_nav do
  = render :partial => 'shared/subnav/events_subnav'

#col_left
  -# HACK:: :page set to false to disable caching since this is the standard events list
  = render :partial => 'shared/events', :locals => { :events => @events, :page => false, :paginate => @paginate }

#col_right
  = render :partial => 'shared/sidebar/top_events', :locals => { :top_events => @top_events }
  = render :partial => 'shared/sidebar/newest_events', :locals => { :newest_events => @newest_events }
