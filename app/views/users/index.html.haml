- content_for :sub_nav do
  = render_ad(:leaderboard, @ad_layout, @ad_leaderboard)
  = render :partial => 'shared/subnav/users_subnav'

#left-col
  = render_ad(:banner, @ad_layout, @ad_banner)
  = render :partial => 'shared/users', :locals => { :scores => @scores, :paginate => @paginate, :page => @page }

#right-col
  = render_ad(:small_square, @ad_layout, @ad_small_square)
  -#= render :partial => 'shared/sidebar/invite_users'
  = render :partial => 'shared/sidebar/active_users'
  = render :partial => 'shared/sidebar/newest_users'
  = render_ad(:skyscraper, @ad_layout, @ad_skyscraper)
