- content_for :head_content do
  :javascript
    $(function() {
    	$('ul.view-objects a').click(function(event) {
    		event.preventDefault();

    		console.log("Loading view object at: "+$(this).attr('href'));
    		$("#output").load($(this).attr('href'));
      });
    });


#left-col
  .panel-2
    .panel-bar
      %h1 View Objects
    .item-list-wrap
      .item-list
        %ul.view-objects
          - @view_objects.each do |view_object|
            %li= link_to view_object.name, view_object_path(view_object, :format => 'json')

#home-content
  #output
